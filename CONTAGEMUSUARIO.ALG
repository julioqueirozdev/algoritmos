algoritmo "contagemUsuario"
var
   contador, n1: inteiro
inicio
   Escreva("Vamos contar até o número: ")
   Leia(n1)
   Escreval ("Vamos contar de 0 até ", n1)
   contador <- 0
   Enquanto (contador <= n1) faca
      Escreval (contador)
      contador <- contador + 1
   fimenquanto
   Escreva ("Acabei, ta passada?")
fimalgoritmo